#include <vector>
#include <iostream>
using namespace std;

class Solution {
public:
    int removeDuplicates(vector<int>& nums) 
    {
        if (nums.size() <= 2) 
        {
            for (int i = 0; i < nums.size(); i++) 
            {
                cout << nums[i] << " ";
            }
            cout << endl;
            return nums.size();
        }

        int k = 2;

        for (int i = 2; i < nums.size(); i++) 
        {
            if (nums[i] != nums[k - 2]) 
            {
                nums[k] = nums[i];
                k++;
            }
        }
        for (int i = 0; i < k; i++) 
        {
            cout << nums[i] << " ";
        }
        cout << endl;

        return k;
    }
};
